#!/usr/bin/expect -f
if {$argc != 1} { 
    puts "Usage: $argv0 IP" 
    exit -1
} else { 
    # set ip [lrange $argv 0 1 ]
    # puts "$ip is the HOST"
    # spacial [] {} etc. can't be put after telnet
    spawn telnet $argv 24
    expect  "login:"
    send    "root\n"

    interact {
        timeout 30 { send "" }
    }
} 
